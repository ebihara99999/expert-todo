{
  "name": "expert-todo",
  "private": true,
  "dependencies": {
    "@rails/webpacker": "3.5",
    "aws-sdk": "^2.171.0",
    "axios": "^0.17.1",
    "coffeescript": "1.12.7",
    "dotenv": "^4.0.0",
    "vue": "^2.4.4",
    "vue-loader": "14.2.2",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "^2.4.4",
    "vuetify": "^0.17.6",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.12",
    "babel-jest": "^22.4.1",
    "babel-loader": "^7.1.4",
    "eslint": "^4.14.0",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-vue": "^4.0.0",
    "jest": "^22.4.2",
    "jest-serializer-vue": "^0.3.0",
    "vue-jest": "^2.1.1",
    "webpack-dev-server": "2.11.2"
  },
  "scripts": {
    "lint": "./node_modules/.bin/eslint app/javascript/components/**/*.vue app/javascript/packs/**/*.js",
    "test": "jest"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ],
    "mapCoverage": true,
    "testMatch": [
      "<rootDir>/__tests__/**/*.spec.js"
    ]
  }
}
